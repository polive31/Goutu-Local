/*
Structure and Layout
---------------------------------------------------------------------------------------------------- */

/* Site Containers
--------------------------------------------- */

.site-container {
	margin:0 auto;
	max-width:1140px;
	padding:0 30px;
}

.site-inner {
	clear:both;
	display:block;
	margin:20px auto 40px auto; /* P.O. original 40px auto*/
}

.logged-in .site-inner {
	margin-top:10px; /* P.O. original 40px auto*/
}
	
.wrap {
	display:block;
	margin:0 auto;
}

/* Column Widths and Positions
--------------------------------------------- */

/* Wrapping div for .content and .sidebar-primary */

.content-sidebar-sidebar .content-sidebar-wrap,
.sidebar-content-sidebar .content-sidebar-wrap,
.sidebar-sidebar-content .content-sidebar-wrap {
	width:840px;
}

.content-sidebar-sidebar .content-sidebar-wrap {
	float:left;
}

.sidebar-content-sidebar .content-sidebar-wrap,
.sidebar-sidebar-content .content-sidebar-wrap {
	float:right;
}

/* Content */

.content {
	float:right;
	width:680px;
}

.content-sidebar .content,
.content-sidebar-sidebar .content,
.sidebar-content-sidebar .content {
	float:left;
}

.content-sidebar-sidebar .content,
.sidebar-content-sidebar .content,
.sidebar-sidebar-content .content {
	width:500px;
}

.full-width-content .content {
	width:100%;
}

/* Primary Sidebar */

.sidebar-primary {
	float:right;
	width:300px;
	display:-webkit-box;      /* OLD - iOS 6-, Safari 3.1-6 */
  display:-moz-box;         /* OLD - Firefox 19- (buggy but mostly works) */
  display:-ms-flexbox;      /* TWEENER - IE 10 */
  display:-webkit-flex;     /* NEW - Chrome */
  display:flex;             /* NEW, Spec - Opera 12.1, Firefox 20+ */ 
	-webkit-flex-flow:row wrap;
	-moz-flex-flow:row wrap;
	-ms-flex-flow:row wrap;
	flex-flow:row wrap;
	-webkit-justify-content:space-around;
	justify-content:space-around;
	-webkit-align-items:flex-start;
	align-items:flex-start;
}

.sidebar-content .sidebar-primary,
.sidebar-sidebar-content .sidebar-primary {
	float:left;
}

.sidebar,
.home.page .sidebar {
	margin-top:0px;
}

.search .sidebar {
	margin-top:20px;
}

.page .sidebar {
	margin-top:40px;
}

.single-recipe .sidebar {
	margin-top:90px;
}

.single-post .sidebar,
.page-template-blogs .sidebar {
	margin-top:60px;
}

.index-page .sidebar {
/*	margin-top:180px;*/
}


/* Secondary Sidebar */

.sidebar-secondary {
	float:left;
	width:200px; /* P.O. Original 160px */
}

.content-sidebar-sidebar .sidebar-secondary {
	float:right;
}

/* Column Classes
	Link:http://twitter.github.io/bootstrap/assets/css/bootstrap-responsive.css
--------------------------------------------- */

.simple-grid {
	float:left;
}


.rpwe-block.grid-6col li,
.simple-grid.one-sixth {
  width:15%; 
}

.rpwe-block.grid-5col li,
.simple-grid.one-fifth {
  width:20%;             
}

.rpwe-block.grid-4col li,
.simple-grid.one-fourth {
  width:25%;            
}

.rpwe-block.grid-3col li,
.simple-grid.one-third {
  width:33.3%; 								
}

.rpwe-block.grid-2col li,
.simple-grid.one-half {
  width:50%; 									
}

.rpwe-block.grid-1col li {
  width:100%; 									 
}

.first {
	clear:both;
	margin-left:0;
}


/* Menus
--------------------------------------------- */

/* Mon Goûtu image menu */
.img-menu {
	float:left;
	margin-bottom:20px;
}

.menu-2col {
	width:50%;
}

.img-menu a {
  display:flex;
  flex-direction:column;
  padding:20px;
  max-width:350px;
  text-decoration:none;
}

.img-menu a:hover {
	background:#e7cea7;
}

.img-menu a img {
	/*width:250px;*/
	height:250px;
	object-fit:contain;
}

.img-menu a .menu-title {
	display:-webkit-box;      /* OLD - iOS 6-, Safari 3.1-6 */
  display:-moz-box;         /* OLD - Firefox 19- (buggy but mostly works) */
  display:-ms-flexbox;      /* TWEENER - IE 10 */
  display:-webkit-flex;     /* NEW - Chrome */
  display:flex;             /* NEW, Spec - Opera 12.1, Firefox 20+ */ 
}

.img-menu a .menu-title span {
	color:#010101;
	font-size:12px;
	font-weight:600;
	letter-spacing:2px;
	padding:3px 8px;
	text-align:center;
	text-transform:uppercase;
	margin:auto;
}


/* PC SCREEN MENU */

.fa-bell.mega-menu-link {
	position:relative;
	padding:10px;
}

a.mega-menu-link img {
  margin-bottom:0px;
}

span.bp_counter {
	font-size:12px;
	font-weight:bold;
	line-height:12px;
	background-color:red;
	border-radius:50%;
  -moz-border-radius:50%;
  -webkit-border-radius:50%;
	color:#FFF;
	text-align:center;
	position:absolute;
	top:3px;
	right:0px;
}

.mega-menu-item span.bp_counter {
	width:18px;
	height:18px;
	padding:3px 5px 2px 5px;
}


/* TOGGLE MENU RESPONSIVE */

/* Counters */

.toggle-menu {
	position:relative;
}

.toggle-menu span.bp_counter {
	width:16px;
	height:16px;
	padding:2px 2px 2px 2px;
	top:-2px;
	right:-8px;
}

/* Icons */

.mega-menu-toggle a {
	text-decoration:none;
	font-size:18px;
}

.dashicons-profile:before {
	content:"\f110";
}

.fa-notifications:before {
    content:"\f0f3"; /*Bell */
}

.fa-friendships:before {
    content:"\f0c0"; /*Friends */
}

.fa-events:before {
